{
  "name": "ml_exploro_vis",
  "productName": "ML ExploroVis",
  "main": "public/electron.js",
  "homepage": "./",
  "description": "TODO",
  "author": "TODO",
  "build": {
    "extraResources": [
      "./python/data/",
      "./python/build/"
    ]
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cross-env": "^7.0.2",
    "d3": "^6.2.0",
    "electron-is-dev": "^1.2.0",
    "fetch-retry": "^4.0.1",
    "node-sass": "4.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^5.0.3",
    "react-spinners": "^0.9.0",
    "reactjs-popup": "^2.0.4"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "python3 python/build.py && pyinstaller python/main.py --distpath python/build && yarn react-build && electron-builder --publish=always && rm -rf python/build",
    "all": "python3 python/build.py && pyinstaller python/main.py --distpath python/build && yarn react-build && electron-builder -mwl && rm -rf python/build",
    "build": "python3 python/build.py && pyinstaller python/main.py --distpath python/build && yarn react-build && yarn electron-build && rm -rf python/build",
    "start": "python3 python/build.py && concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/d3": "^6.2.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "concurrently": "^5.3.0",
    "electron": "^11.0.3",
    "electron-builder": "^22.9.1",
    "electron-rebuild": "^2.3.2",
    "node-gyp": "^7.1.2",
    "react-scripts": "4.0.1",
    "typescript": "^4.0.3",
    "wait-on": "^5.2.0",
    "web-vitals": "^0.2.4"
  }
}
